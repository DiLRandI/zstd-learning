{
  "uid": "compress-decompress-overview",
  "title": "Compression / Decompression Overview",
  "description": "Zstd compression and decompression metrics",
  "tags": ["zstd", "compression", "decompression"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 2,
  "refresh": "10s",
  "templating": {
    "list": [
      {
        "name": "run_id",
        "type": "query",
        "datasource": "Prometheus",
        "refresh": 1,
        "query": "label_values(compress_duration_seconds, run_id)",
        "includeAll": true,
        "multi": true,
        "allValue": ".*",
        "current": {
          "text": "All",
          "value": "$__all"
        }
      },
      {
        "name": "source",
        "type": "query",
        "datasource": "Prometheus",
        "refresh": 1,
        "query": "label_values(compress_duration_seconds, source)",
        "includeAll": true,
        "multi": true,
        "allValue": ".*",
        "current": {
          "text": "All",
          "value": "$__all"
        }
      },
      {
        "name": "level",
        "type": "query",
        "datasource": "Prometheus",
        "refresh": 1,
        "query": "label_values(compress_duration_seconds, level)",
        "includeAll": true,
        "multi": true,
        "allValue": ".*",
        "current": {
          "text": "All",
          "value": "$__all"
        }
      },
      {
        "name": "use_dict",
        "type": "query",
        "datasource": "Prometheus",
        "refresh": 1,
        "query": "label_values(compress_duration_seconds, use_dict)",
        "includeAll": true,
        "multi": true,
        "allValue": ".*",
        "current": {
          "text": "All",
          "value": "$__all"
        }
      }
    ]
  },
  "panels": [
    {
      "type": "text",
      "title": "Run Filters",
      "options": {
        "mode": "markdown",
        "content": "**Run ID:** $run_id  |  **Source:** $source  |  **Level:** $level  |  **Dict:** $use_dict"
      },
      "gridPos": { "h": 3, "w": 24, "x": 0, "y": 0 }
    },
    {
      "type": "stat",
      "title": "Compress Last Run (Age)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "time() - compress_last_run_timestamp_seconds{run_id=~\"$run_id\",source=~\"$source\",level=~\"$level\",use_dict=~\"$use_dict\"}",
          "refId": "A",
          "instant": true,
          "legendFormat": "{{run_id}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 1
        }
      },
      "gridPos": { "h": 5, "w": 8, "x": 0, "y": 3 }
    },
    {
      "type": "stat",
      "title": "Compress Duration",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "compress_duration_seconds{run_id=~\"$run_id\",source=~\"$source\",level=~\"$level\",use_dict=~\"$use_dict\"}",
          "refId": "A",
          "instant": true,
          "legendFormat": "{{run_id}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3
        }
      },
      "gridPos": { "h": 5, "w": 8, "x": 8, "y": 3 }
    },
    {
      "type": "stat",
      "title": "Compress Ratio",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "compress_ratio{run_id=~\"$run_id\",source=~\"$source\",level=~\"$level\",use_dict=~\"$use_dict\"}",
          "refId": "A",
          "instant": true,
          "legendFormat": "{{run_id}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ratio",
          "decimals": 3
        }
      },
      "gridPos": { "h": 5, "w": 8, "x": 16, "y": 3 }
    },
    {
      "type": "stat",
      "title": "Compress Input Bytes",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "compress_input_bytes{run_id=~\"$run_id\",source=~\"$source\",level=~\"$level\",use_dict=~\"$use_dict\"}",
          "refId": "A",
          "instant": true,
          "legendFormat": "{{run_id}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "decimals": 0
        }
      },
      "gridPos": { "h": 5, "w": 8, "x": 0, "y": 8 }
    },
    {
      "type": "stat",
      "title": "Compress Output Bytes",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "compress_output_bytes{run_id=~\"$run_id\",source=~\"$source\",level=~\"$level\",use_dict=~\"$use_dict\"}",
          "refId": "A",
          "instant": true,
          "legendFormat": "{{run_id}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "decimals": 0
        }
      },
      "gridPos": { "h": 5, "w": 8, "x": 8, "y": 8 }
    },
    {
      "type": "stat",
      "title": "Compress Files",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "compress_files_processed{run_id=~\"$run_id\",source=~\"$source\",level=~\"$level\",use_dict=~\"$use_dict\"}",
          "refId": "A",
          "instant": true,
          "legendFormat": "{{run_id}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0
        }
      },
      "gridPos": { "h": 5, "w": 8, "x": 16, "y": 8 }
    },
    {
      "type": "stat",
      "title": "Decompress Last Run (Age)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "time() - decompress_last_run_timestamp_seconds{run_id=~\"$run_id\",source=~\"$source\",use_dict=~\"$use_dict\"}",
          "refId": "A",
          "instant": true,
          "legendFormat": "{{run_id}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 1
        }
      },
      "gridPos": { "h": 5, "w": 8, "x": 0, "y": 13 }
    },
    {
      "type": "stat",
      "title": "Decompress Duration",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "decompress_duration_seconds{run_id=~\"$run_id\",source=~\"$source\",use_dict=~\"$use_dict\"}",
          "refId": "A",
          "instant": true,
          "legendFormat": "{{run_id}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3
        }
      },
      "gridPos": { "h": 5, "w": 8, "x": 8, "y": 13 }
    },
    {
      "type": "stat",
      "title": "Decompress Ratio",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "decompress_ratio{run_id=~\"$run_id\",source=~\"$source\",use_dict=~\"$use_dict\"}",
          "refId": "A",
          "instant": true,
          "legendFormat": "{{run_id}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ratio",
          "decimals": 3
        }
      },
      "gridPos": { "h": 5, "w": 8, "x": 16, "y": 13 }
    },
    {
      "type": "stat",
      "title": "Decompress Input Bytes",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "decompress_input_bytes{run_id=~\"$run_id\",source=~\"$source\",use_dict=~\"$use_dict\"}",
          "refId": "A",
          "instant": true,
          "legendFormat": "{{run_id}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "decimals": 0
        }
      },
      "gridPos": { "h": 5, "w": 8, "x": 0, "y": 18 }
    },
    {
      "type": "stat",
      "title": "Decompress Output Bytes",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "decompress_output_bytes{run_id=~\"$run_id\",source=~\"$source\",use_dict=~\"$use_dict\"}",
          "refId": "A",
          "instant": true,
          "legendFormat": "{{run_id}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "decimals": 0
        }
      },
      "gridPos": { "h": 5, "w": 8, "x": 8, "y": 18 }
    },
    {
      "type": "stat",
      "title": "Decompress Files",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "decompress_files_processed{run_id=~\"$run_id\",source=~\"$source\",use_dict=~\"$use_dict\"}",
          "refId": "A",
          "instant": true,
          "legendFormat": "{{run_id}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0
        }
      },
      "gridPos": { "h": 5, "w": 8, "x": 16, "y": 18 }
    }
  ],
  "time": { "from": "now-24h", "to": "now" }
}
