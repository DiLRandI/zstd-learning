{
  "uid": "generator-overview",
  "title": "Generator Overview",
  "description": "Generator metrics",
  "tags": ["generator"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 2,
  "refresh": "10s",
  "panels": [
    {
      "type": "stat",
      "title": "Last Run (Age)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "time() - last_run_timestamp_seconds",
          "refId": "A",
          "instant": true,
          "legendFormat": "{{type}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 1
        }
      },
      "gridPos": { "h": 5, "w": 8, "x": 0, "y": 0 }
    },
    {
      "type": "stat",
      "title": "Last Run Duration",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "generate_duration_seconds",
          "refId": "A",
          "instant": true,
          "legendFormat": "{{type}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3
        }
      },
      "gridPos": { "h": 5, "w": 8, "x": 8, "y": 0 }
    },
    {
      "type": "stat",
      "title": "Items Generated (Last Run)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "generated_items_total",
          "refId": "A",
          "instant": true,
          "legendFormat": "{{type}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0
        }
      },
      "gridPos": { "h": 5, "w": 8, "x": 16, "y": 0 }
    },
    {
      "type": "timeseries",
      "title": "Generated Items Over Time",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "generated_items_total",
          "refId": "A",
          "legendFormat": "{{type}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0
        }
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 5 }
    }
  ],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" }
}
